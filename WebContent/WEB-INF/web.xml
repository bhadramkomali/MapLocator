<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MapLocator</display-name>
  
  <!-- FeedData servlet -->
  <servlet>
    <servlet-name>FeedDataServlet</servlet-name>
    <servlet-class>com.swarm.servlets.FeedDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FeedDataServlet</servlet-name>
    <url-pattern>/feeddata.do</url-pattern>
  </servlet-mapping>
  
  <!-- ViewInMap servlet -->
  <servlet>
  	<servlet-name>ViewInMapServlet</servlet-name>
  	<servlet-class>com.swarm.servlets.ViewInMapServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ViewInMapServlet</servlet-name>
  	<url-pattern>/viewinmap.do</url-pattern>
  </servlet-mapping>
  
</web-app>